"Capitulos":[{
	"Capitulo": [{
		"id": 0,
		"Escena":[{
			"id": 0,
			"Mensajes":[{
				"id":0,
				"Cadena": "¡Hola! ¿Te has caído y te has quedado atrapado aquí?
							Pues ya somos dos.
							Cuando entré aquí una piedra me bloqueó el camino.
							Veo que eres un animal con manos como MOVI, ¿me ayudas a romperla?",
				"Audio": "Sound/VoiceOver/Chapter1/Episode1/clip1",
				"x": 2.5,
				"y": 30.5,
				"Duracion": 14,
				"Silencio":1},
				{
				"id":1,
				"Cadena": "¡Muy bien! Un problema menos. Ahora queda salir de la zona prohibida.
							Ven, te presentaré a MOVI, si es capaz de quedarse quieta cinco segundos.",
				"Audio": "Sound/VoiceOver/Chapter1/Episode1/clip2",
				"x": 2.5,
				"y": 30.5,
				"Duracion": 9,
				"Silencio":1}],

			"id": 1,
			"Mensajes":[{
				"id":0,
				"Cadena": "¡Movi, por fin te encuentro!",
				"Audio": "Sound/VoiceOver/Chapter1/Episode1/clip1",
				"x": 2.5,
				"y": 30.5,
				"Duracion": 2,
				"Silencio":1},
				{
				"id":1,
				"Cadena": "¡ONE!¿Dónde te habías metido? Estaba hablando contigo.",
				"Audio": "Sound/VoiceOver/Chapter1/Episode1/clip2",
				"x": 2.5,
				"y": 30.5,
				"Duracion": 4.5,
				"Silencio":1},
				{
				"id":2,
				"Cadena": "Me había quedado encerrado...
							¿Que estas haciendo?",
				"Audio": "Sound/VoiceOver/Chapter1/Episode1/clip2",
				"x": 2.5,
				"y": 30.5,
				"Duracion": 3.5,
				"Silencio":1},
				{
				"id":3,
				"Cadena": "Intento solucionar este rompecabezas,
							seguro que consigo algo interesante si logro abrirlo.",
				"Audio": "Sound/VoiceOver/Chapter1/Episode1/clip2",
				"x": 2.5,
				"y": 30.5,
				"Duracion": 5.5,
				"Silencio":1},
				{
				"id":4,
				"Cadena": "Ayudémosle, si no, no nos va a hacer caso.",
				"Audio": "Sound/VoiceOver/Chapter1/Episode1/clip2",
				"x": 2.5,
				"y": 30.5,
				"Duracion": 3,
				"Silencio":1},
				{
				"id":5,
				"Cadena": "Sabía que había una llave oculta.
							¡Oh, hola! Soy Movi.",
				"Audio": "Sound/VoiceOver/Chapter1/Episode1/clip2",
				"x": 2.5,
				"y": 30.5,
				"Duracion": 5,
				"Silencio":1},
				{
				"id":6,
				"Cadena": "Ah, yo soy One, se me olvidó presentarme.
							Nos quedamos atrapados en la zona prohibida sin querer.",
				"Audio": "Sound/VoiceOver/Chapter1/Episode1/clip2",
				"x": 2.5,
				"y": 30.5,
				"Duracion": 6.5,
				"Silencio":1},
				{
				"id":7,
				"Cadena": "Ahora tenemos una llave, ¡vamos a ver para qué sirve!",
				"Audio": "Sound/VoiceOver/Chapter1/Episode1/clip2",
				"x": 2.5,
				"y": 30.5,
				"Duracion": 4,
				"Silencio":1},
				{
				"id":8,
				"Cadena": "Movi,¡espera!",
				"Audio": "Sound/VoiceOver/Chapter1/Episode1/clip2",
				"x": 2.5,
				"y": 30.5,
				"Duracion": 1.5,
				"Silencio":1}],

			"id":2,
			"Mensajes":[{
				"id":0,
				"Cadena": "Aquí es, hemos llegado.",
				"Audio": "Sound/VoiceOver/Chapter1/Episode1/clip2",
				"x": 2.5,
				"y": 30.5,
				"Duracion": 2,
				"Silencio":1},
				{
				"id":1,
				"Cadena": "Sí, de ahí arriba nos caímos y no hemos sabido volver a subir.",
				"Audio": "Sound/VoiceOver/Chapter1/Episode1/clip2",
				"x": 3,
				"y": 30.5,
				"Duracion": 3,
				"Silencio":1},
				{
				"id":2,
				"Cadena": "Seguro que estas cosas en la pared sirven para algo. Vamos a investigar.",
				"Audio": "Sound/VoiceOver/Chapter1/Episode1/clip2",
				"x": 2.5,
				"y": 30.5,
				"Duracion": 4,
				"Silencio":1},
				{
				"id":3,
				"Cadena": "¡Já!¡Te dije que eso servía para algo! ¡Vamos, corred!",
